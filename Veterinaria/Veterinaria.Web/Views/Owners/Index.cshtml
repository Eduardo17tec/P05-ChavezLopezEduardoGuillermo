@model IEnumerable<Veterinaria.Web.Models.Owner>

@{
    ViewBag.Title = "Index";
}

<h2>Dueños</h2>

<a href="@Url.Action("Create","Owners")" class="btn btn-sm btn-success">
    <span class="text-white-50"><i class="fas fa-plus-circle"></i></span>
    <span class="text-white">Añadir nuevo dueño</span>
</a>
<table class="table table-dark">
    <tr>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
